{
  "name": "SafeGuard",
  "description": "Multi-Tool Google Chrome Extension for Web Development",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },

  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },

  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ]


}
